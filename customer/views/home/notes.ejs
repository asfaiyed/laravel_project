<!DOCTYPE html>
<html>
<head>
	<title>UPLOADED Product</title>
	

</head>
<body>
	

	
	<fieldset>
	<legend><h1>UPLOADED Product</h1></legend>

	<table border="1">
		<tr>
			<th>id</th>
			<th>Product Name</th>
			<th>filename</th>
			<th>operation</th>
		
		</tr>
      <% for(var i=0; i < results.length; i++){ %>
		
		<tr>
			
			<td><%= results[i].id %></td>
			<td><%= results[i].product_name %></td>
			<td><a href='/assets/<%= results[i].filename %>'><%= results[i].filename %></a></td>
			
			<td><a href="/home/upload/notes/delete/<%= results[i].id %>"> Delete </a></td>
		</tr>
		<% } %>
	</table>
	<br/>
	<a href="/home/upload">Back</a>
	<a href="/home">Go to Home Page</a>
	</fieldset
</body>
</html>
