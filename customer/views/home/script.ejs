<!DOCTYPE html>
<html>
<head>
	<title>ANSWERS</title>
			<style type="text/css">	
	table{
	border-collapse: collapse;
	width: 100%;
	color: #d96459;
	font-family: monospace;
	font-size: 25px;
	text-align: left;
	}
	th{
		background-color: #d96459;
		color: white;
		}
		tr:nth-child(even) {background-color: #f2f2f2;}
 
</style>
	
</head>
<body>
<fieldset>

<h1>ANSWERS</h1>
<table border="1">
		<tr>
			<th>id</th>
			<th>name</th>
			<th>question</th>
		
			<th>reply</th>
		
		</tr>
      <% for(var i=0; i < results.length; i++){ %>
		
		<tr>
			<td><%= results[i].id %></td>
			<td><%= results[i].name %></td>
			<td><%= results[i].question %></td>
		
			<td><%= results[i].reply %></td>
			
		</tr>
		<% } %>
	</table>
	<br/>
	
		<a href="/home">Back</a>
		<br/><br/>
	
		
			
		</fieldset>
</body>
</html>